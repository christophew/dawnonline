<Window x:Class="FrontEnd.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="366" Width="632" Loaded="Window_Loaded" KeyDown="Window_KeyDown" MouseWheel="Window_MouseWheel">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="222*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Canvas Name="MyCanvas" Grid.Row="1" Grid.ColumnSpan="4">
        </Canvas>
        <Button Grid.Column="0" Click="AddPlants_Click">Add Plants</Button>
        <Button Grid.Column="1" Click="AddRabbits_Click">Add Rabbits</Button>
        <Button Grid.Column="2" Click="AddPredators_Click">Add Predators</Button>
        <Label Grid.Column="3" HorizontalAlignment="Left" Name="Info" ></Label>
    </Grid>
</Window>
